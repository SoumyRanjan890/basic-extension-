{
    "manifest_version": 3,
    "name": "Hello World Extension",
    "version": "1.0",
    "description": "A simple Hello World Chrome Extension",
    "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "image.png"
    }
  }
  